ctive(key) {\n","          const element = document.querySelector('#df-7c05c549-e63c-44d3-afd6-40ff2f0c4892');\n","          const dataTable =\n","            await google.colab.kernel.invokeFunction('convertToInteractive',\n","                                                     [key], {});\n","          if (!dataTable) return;\n","\n","          const docLinkHtml = 'Like what you see? Visit the ' +\n","            '<a target=\"_blank\" href=https://colab.research.google.com/notebooks/data_table.ipynb>data table notebook</a>'\n","            + ' to learn more about interactive tables.';\n","          element.innerHTML = '';\n","          dataTable['output_type'] = 'display_data';\n","          await google.colab.output.renderOutput(dataTable, element);\n","          const docLink = document.createElement('div');\n","          docLink.innerHTML = docLinkHtml;\n","          element.appendChild(docLink);\n","        }\n","      </script>\n","    </div>\n","  </div>\n","  "]},"metadata":{},"execution_count":9}]},{"cell_type":"markdown","source":["# C2.Calcule la entropía de Shannon (base 2) de la pertenencia a las categorías de los negocios."],"metadata":{"id":"Xj5IyuFb1LqJ"}},{"cell_type":"code","source":["import pandas as pd\n","import numpy as np\n","\n","datos = pd.read_csv('/content/drive/MyDrive/Colab Notebooks/Copia de business.csv',usecols = ['attributes'] )\n","datos.dropna(inplace = True)\n","\n","\n","SinRangoDePrecio = datos.loc[~datos['attributes'].str.contains(\"\"\"'RestaurantsPriceRange2':\"\"\"), :].count()#51288\n","\n","Precio1 = datos.loc[datos['attributes'].str.contains(\"\"\"'RestaurantsPriceRange2': '1'\"\"\"), :].count()#28840\n","\n","Precio2 = datos.loc[datos['attributes'].str.contains(\"\"\"'RestaurantsPriceRange2': '2'\"\"\") , :].count()#48581\n","\n","Precio3 = datos.loc[datos['attributes'].str.contains(\"\"\"'RestaurantsPriceRange2': '3'\"\"\"), :].count()#6667\n","\n","Precio4 = datos.loc[datos['attributes'].str.contains(\"\"\"'RestaurantsPriceRange2': '4'\"\"\"), :].count()#1192\n","\n","\n","Total = Precio1 +Precio2 + Precio3 + Precio4 + SinRangoDePrecio\n","\n","Porcentaje0 = SinRangoDePrecio/Total\n","Porcentaje1 = Precio1/Total\n","Porcentaje2 = Precio2/Total\n","Porcentaje3 = Precio3/Total\n","Porcentaje4 = Precio4/Total\n","Entropia_De_Shannon = -(Porcentaje1 * np.log2(Porcentaje1) + Porcentaje2 * np.log2(Porcentaje2) + Porcentaje3 * np.log2(Porcentaje3)+ Porcentaje4 * np.log2(Porcentaje4) + Porcentaje0 * np.log2(Porcentaje0)  )\n","print('La Entropia de Shannon para los distintos rangos de precios resulta ser: ',float(Entropia_De_Shannon))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"dZs-8cmI1u9B","executionInfo":{"status":"ok","timestamp":1651338916764,"user_tz":180,"elapsed":2659,"user":{"displayName":"Nico Allende","userId":"09334469959637297675"}},"outputId":"854c76d3-727c-48da-9495-c4a90d17dd4f"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["La Entropia de Shannon para los distintos rangos de precios resulta ser:  1.807214003644265\n"]}]},{"cell_type":"markdown","source":["# V9.Consiga exactamente esta visualización utilizando los datos. Consideramos que un negocio está en una ubicación compartida si su dirección es usada por más de un negocio. \n","\n","![image.png](data:image/png;base64,)"],"metadata":{"id":"--1DkYp1NB2v"}},{"cell_type":"code","source":["from numpy.core.fromnumeric import size\n","from locale import normalize\n","import pandas as pd\n","import seaborn as sns\n","import matplotlib.pyplot as plt\n","\n","\n","palette_tab10 = sns.color_palette(\"Accent\", 10)\n","palette_paired = sns.color_palette(\"tab10\", 10)\n","paleta = sns.color_palette([palette_paired[9], palette_tab10[5]])\n","##################################################hasta aca es para los colores de las visus no tocar\n","\n","datosNegocio = pd.read_csv('/content/drive/MyDrive/Colab Notebooks/Copia de business.csv', usecols=['address','business_id'])\n","\n","#aca consigo una columna que me dice si el lugar es compartido o no \n","listaConlocales = datosNegocio.groupby(['address'])['business_id'].transform('count')\n","datosNegocio['Ubicación'] = (listaConlocales > 1)\n","\n","datosReviews = pd.read_csv('/content/drive/MyDrive/Colab Notebooks/Copia de review.csv', usecols =['stars', 'business_id' ])\n","\n","\n","#aca mergeo para poder tener la columna de si esta compartido o no los locales junto con las estrellas de las  distintas reviews\n","nuevoDFdeDatos = datosReviews.merge(datosNegocio, left_on ='business_id', right_on= 'business_id', how='outer')\n","\n","agrupado = nuevoDFdeDatos.groupby(['Ubicación'])['stars'].value_counts(normalize = True).rename('Proporción').to_frame()\n","agrupado.sort_values(['stars'], inplace=True)\n","\n","agrupado = agrupado.rename(index={True: 'Compartido', False: 'Local único'}).reset_index()\n","agrupado = agrupado.set_index(['stars'])\n","agrupado = agrupado.rename({'Compartido' : '★' , 2.0 : '★★', 3.0 : '★★★', 4.0 : '★★\\n★★', 5.0 : '★★★\\n★★'}).reset_index()\n","\n","\n","fig = plt.figure(figsize= (6.4, 4.4),dpi =150)\n","ax = fig.add_subplot(111)\n","sns.barplot( data = agrupado , x='stars', y = 'Proporción', hue = 'Ubicación', palette= paleta, )\n","plt.title('Distribución de las estrellas de las reviews \\n según donde esta el negocio')\n","plt.ylabel('Proporción');\n","ax.tick_params(axis='x', colors='#e0c85a', labelsize = 17)\n","plt.ylim(top =1)\n","plt.xlabel('Estrellas')\n","plt.show()"],"metadata":{"id":"7BhSY1kSM968","colab":{"base_uri":"https://localhost:8080/","height":722},"executionInfo":{"status":"ok","timestamp":1651339092298,"user_tz":180,"elapsed":42734,"user":{"displayName":"Nico Allende","userId":"09334469959637297675"}},"outputId":"41d4d9f8-0328-4acc-8797-9ae63d475a37"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 960x660 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdow